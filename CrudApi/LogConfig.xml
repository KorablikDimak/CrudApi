<?xml version="1.0" encoding="utf-8"?>
<logger>
    <targets>
        <target logsender="File" minlevel="Warning" filepath="{basedir}\logs\{shortdate}.log"
                layout="{shortdate} | {longtime}:{millisecond} | {level} | {class} {method}() | line: {line} | {message}"/>
        <!--<target logsender="Database" minlevel="Error" connectionstring="На данный момент моя библиотека поддерживает подключение только к MsSql"
                layout="{shortdate} | {longtime}:{millisecond} | {level} | {class} {method}() | line: {line} | {message}"
                tablename="authlogs"/>-->
    </targets>
</logger>